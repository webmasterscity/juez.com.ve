CREATE TABLE uptp4 (
    solucion VARCHAR(50),
    id_sol integer(1)
    );
CREATE TABLE uptp5 (
    solucion VARCHAR(50),
    id_sol integer(1)
    );
    
INSERT INTO uptp4 (solucion,1) VALUES('Hola');
INSERT INTO uptp5 (solucion,1) VALUES('Mundo!');
SELECT CONCAT(uptp4.solucion,' ',uptp5.solucion) AS solu FROM uptp4 INNER JOIN uptp5 ON(uptp4.id_sol=uptp5.id_sol)